{
    "manifest_version": 2,
    "name": "one crawly boi",
    "version": "3.5.11",
    "description": "",
    "icons": {
        "128": "images/icon.png"
    },
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": ["js/content.js"],
        "run_at": "document_idle",
        "all_frames": false
    }],
    "minimum_chrome_version": "73",
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}